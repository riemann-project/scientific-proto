<h1>ProblemImages#edit</h1>
<p>Find me in app/views/problem_images/edit.html.erb</p>

<%= image_tag @image.image %><br>
<%= link_to "delete", problem_problem_image_path(@image.problem, @image), method: :delete %>

<%= form_tag("./", :method => "PUT") do %><br>
  x<%= number_field_tag "x" %><br>
  y<%= number_field_tag "y" %><br>
